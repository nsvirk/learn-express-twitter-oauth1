<p>
  ID: <%= user.id %><br />
    Username: <%= user.username %><br />
      Name: <%= user.displayName %><br />
        <% if (user.emails) { %>
          Email: <%= user.emails[0].value %><br />
            <% } %>
</p>

<% if (!user) { %>
  <p>Welcome! Please <a href="/login">log in</a>.</p>
  <% } else { %>
    <p>You can <a href="/logout">logout</a>.</p>
    <% } %>